                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="10dp"
                    android:layout_weight="0.2"
                    android:gravity="center_vertical"
                    android:text="@string/city_QuanGuoJiChu"
                    android:textColor="@color/listColor1"
                    android:textSize="17dp" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.7"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/cityQuanGuoJiChuTextView"
                        android:layout_width="50dp"
                        android:layout_height="0dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp"
                        android:layout_weight="0.7"
                        android:gravity="center"
                        android:hint="@string/btn_download"
                        android:textColor="@color/listColor1"
                        android:visibility="gone"
                        android:textSize="12dp" />

                    <ProgressBar
                        android:id="@+id/cityQuanGuoJiChuProgressbar"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_weight="0.1"
                        android:visibility="gone"
                        android:max="100"
                        android:progress="0" />
                </LinearLayout>

                <Button
                    android:id="@+id/cityQuanGuoJiChuDown"
                    android:layout_width="7dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="10dp"
                    android:layout_weight="0.1"
                    android:background="@drawable/list_citymap_load"
                    android:clickable="true"
                    android:gravity="center"
                     android:textSize="11dp"
                    android:textColor="@color/listColor1" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/solidline" />
            

            <!-- 上海下载 -->

